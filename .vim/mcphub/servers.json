        "parrot"
      ],
      "disabled_resources": [
        
      ]
    },
    "mcphub": {
      "custom_instructions": {
        "disabled": true
      },
      "disabled": false,
      "disabled_prompts": [
        "create_native_server"
      ],
      "disabled_resources": [
        "mcphub://changelog",
        "mcphub://native_server_guide",
        "mcphub://docs"
      ]
    }
  }
}