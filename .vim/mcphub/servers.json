    "fetch": {
      "args": [
        "C:/mcp-servers/fetch-mcp/dist/index.js"
      ],
      "disabled": true,
      "command": "node",
      "disabled_tools": [
        
      ]
    },
    "sequential-thinking": {
      "args": [
