      ],
      "disabled": true,
      "command": "pnpm"
    },
    "memory": {
      "args": [
        "dlx",
        "@modelcontextprotocol/server-memory"
      ],
      "disabled": true,
      "command": "pnpm"
    },
    "git": {
      "args": [
        "-m",
        "mcp_server_git"
      ],
