{
  "mcpServers": {
    "DuckDuckGo": {
      "args": [
        "dlx",
        "duckduckgo-mcp-server"
      ],
      "disabled": true,
      "command": "pnpm"
    },
    "time": {
      "args": [
        "-m",
        "mcp_server_time",
        "--local-timezone=Asia/Tokyo"
      ],
      "disabled": true,
      "command": "python"
    },
    "taskmanager": {
      "args": [
